.collapsible {
  background-color:rgb(194, 194, 163);
  color: rgb(46, 46, 31);
  cursor: pointer;
  padding: 18px;
  width: 100%;
  border: none;
  text-align: left;
  outline: none;
	font-family: OCR A Std, monospace;
	font-size: 40px;
}


.active, .collapsible:hover {
  background-color: rgb(245, 245, 240);
}


.NFT {
  padding: 0 18px;
  display: none;
  overflow: hidden;
  background-color: rgb(245, 245, 240);
	font-family: OCR A Std, monospace;
	font-size: 20px;
}

.NFT img
{
	padding-top: 20px;
	padding-bottom: 60px;
}
.collapsible:after { /* unique code*/
  content: '\25BC'; 
  font-size: 13px;
  color: rgb(46, 46, 31);
  float: right;
  margin-left: 5px;
}

.active:after {
  content: "\25AC"; 
	color: rgb(46, 46, 31);
}



